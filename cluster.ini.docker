[global]
xcatgroup=test
basename=test
baseurl=public.hostname.org:8443
maxnodes=40
disksize=20g
repobase=https://github.com/kbase/
deploydir=/kb/deployment
devcontainer=/kb/dev_container
runtime=/kb/runtime
dtdir=/root/dt
hashfile=githashes
